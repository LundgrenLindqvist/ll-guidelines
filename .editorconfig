# EditorConfig makes it easy to maintain text editor configuration between
# multiple developers. See http://editorconfig.org/ for more details

root = true

[*]
indent_style = space
indent_size = 4
trim_trailing_whitespace = true
insert_final_newline = true

[*.md]
trim_trailing_whitespace = false

[{*.{json,md,y{a,}ml},.jshintrc}]
indent_size = 2
